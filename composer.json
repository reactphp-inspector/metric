{
  "name": "react-inspector/metric",
  "description": "\ud83d\udcd0 Metric VO's",
  "license": "MIT",
  "authors": [
    {
      "name": "Cees-Jan Kiewiet",
      "email": "ceesjank@gmail.com",
      "homepage": "https://www.wyrihaximus.net/"
    }
  ],
  "require": {
    "php": "^8 || ^7.4",
    "thecodingmachine/safe": "^1.3.3"
  },
  "require-dev": {
    "wyrihaximus/test-utilities": "^3.7.3"
  },
  "config": {
    "platform": {
      "php": "7.4.7"
    },
    "sort-packages": true
  },
  "extra": {
    "unused": [
      "php"
    ]
  },
  "autoload": {
    "psr-4": {
      "ReactInspector\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ReactInspector\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer normalize"
    ],
    "post-update-cmd": [
      "composer normalize"
    ]
  }
}
